<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="utf-8">
    <title>{% block title %} memberRegist App{% endblock %}</title>
    {% block stylesheets %}
        <link href='http://fonts.googleapis.com/css?family=Irish+Grover' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=La+Belle+Aurore' rel='stylesheet' type='text/css'>
        <link href="{{ asset('css/style.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}
</head>

<body>
<div class ="test">
    <form action="{{ path('user_regist')}}" class="regist" method="post">


        {{ form_errors(form) }}
        {#{{ form_row(form.id) }}#}
        {{ form_row(form.email) }}
        {{ form_rest(form) }}
        <div>
            {{ form_widget(form.serch, {"attr":{"class":"btn btn-primary"}}) }}
        </div>
    </form>


    <div>
        {% for memberitem in memberlist %}
            <div>{{ memberitem.id }}</div>
            <div>{{ memberitem.name }}</div>
            <div>{{ memberitem.furigana }}</div>
            <div>{{ memberitem.email }}</div>
            <div>{{ memberitem.github }}</div>

            <hr>

        {% endfor %}
        {{ msg }}
    </div>

</div>
</body>

</html>

